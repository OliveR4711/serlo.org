{
  "name": "@serlo/athene2-assets",
  "version": "3.7.3",
  "bugs": {
    "url": "https://github.com/serlo-org/athene2-assets/issues"
  },
  "repository": "serlo-org/athene2-assets",
  "license": "Apache-2.0",
  "author": "Serlo Education e.V.",
  "scripts": {
    "build": "npm-run-all build:*",
    "prebuild:assets": "rimraf dist",
    "build:assets": "webpack --config webpack.main.prod.config",
    "build:cf": "lerna run --scope @serlo/cloudflare-workers build",
    "prebuild:gcf": "rimraf dist-gcf",
    "build:gcf": "webpack --config webpack.gcf.config",
    "predeploy": "yarn changelog",
    "deploy": "ts-node scripts/deploy",
    "deploy:assets": "lerna run --stream --scope @serlo/assets deploy",
    "deploy:cf": "lerna run --stream --scope @serlo/cloudflare-workers deploy",
    "deploy-finalize": "run-s deploy-finalize:git sentry-release",
    "deploy-finalize:git": "git tag --force $npm_package_version && git push && git push origin $npm_package_version",
    "sentry-release": "run-s sentry-release:create sentry-release:set-commits sentry-release:deploy",
    "sentry-release:create": "sentry-cli releases new --project athene2-assets athene2-assets@$npm_package_version",
    "sentry-release:set-commits": "sentry-cli releases set-commits --auto athene2-assets@$npm_package_version",
    "sentry-release:deploy": "sentry-cli releases deploys athene2-assets@$npm_package_version new --env production",
    "format": "yarn _prettier --write",
    "lint": "yarn _prettier --check",
    "changelog": "ts-node scripts/changelog",
    "license": "ts-node scripts/license-headers",
    "start": "npm-run-all --parallel start:*",
    "start:packages": "lerna run --parallel start",
    "start:main": "webpack-dev-server --config webpack.main.dev.config",
    "start:editor": "nodemon -w src/ory-editor --exec \"node src/editor/index.server.js\"",
    "test": "jest",
    "_prettier": "prettier \"{{packages/*/,}{.circleci,__tests__,scripts,src}/**/*,*}.{js,jsx,ts,tsx,json,md,yaml,yml,css,scss}\""
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@sentry/browser": "^4.0.0",
    "@serlo/editor": "^0.5.0",
    "algebra.js": "^0.2.0",
    "autosize": "^4.0.0",
    "base-64": "^0.1.0",
    "blueimp-file-upload": "^9.0.0",
    "blueimp-load-image": "^2.0.0",
    "body-parser": "^1.0.0",
    "bootstrap-datepicker": "^1.0.0",
    "bootstrap-sass": "^3.0.0",
    "d3": "^3.0.0",
    "dnode": "^1.0.0",
