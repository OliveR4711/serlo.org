    "howler": "^2.0.0",
    "inquirer": "^6.0.0",
    "jasny-bootstrap": "^3.0.0",
    "jquery": "^3.0.0",
    "jquery-sticky": "^1.0.0",
    "jquery-ui": "^1.0.0",
    "jquery.redirect": "^1.0.0",
    "lodash": "^4.0.0",
    "lodash.debounce": "^4.0.0",
    "magnific-popup": "^1.0.0",
    "moment": "^2.0.0",
    "ramda": "^0.26.0",
