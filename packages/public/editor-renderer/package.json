{
  "name": "@serlo/serlo-org-editor-renderer",
  "version": "2.0.0",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production babel src --extensions \".ts,.tsx\" --out-dir dist --root-mode upward --source-maps",
    "build:dev": "(cd ../../..; npx lerna run --include-filtered-dependencies --scope=$npm_package_name build)",
    "deploy:image": "ts-node scripts/deploy",
    "start": "nodemon --watch src --ext ts,tsx --exec \"yarn build:dev && node dist/index.js\"",
    "test:js": "jest"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@edtr-io/renderer-ssr": "^0.8.0",
    "@serlo/edtr-io": "^0.0.0",
    "@serlo/editor-helpers": "^0.0.0",
    "@serlo/legacy-editor-to-editor": "^0.0.0",
    "babel-runtime": "^6.0.0",
    "body-parser": "^1.0.0",
    "express": "^4.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "styled-components": "^4.0.0"
  },
  "devDependencies": {
    "@serlo/docker": "^0.0.0",
    "@serlo/node-image": "^0.0.0",
    "@serlo/workspace-image": "^0.0.0",
    "@types/express": "^4.0.0",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "@types/styled-components": "^4.0.0",
    "@types/slate-react": "^0.22.5"
  },
  "private": true
}
