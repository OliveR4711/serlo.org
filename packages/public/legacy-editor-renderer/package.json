{
  "name": "@serlo/legacy-editor-renderer",
  "version": "0.1.8",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production babel src --extensions \".ts,.tsx\" --out-dir dist --root-mode upward --source-maps",
    "deploy:image": "ts-node scripts/deploy",
    "start": "nodemon --legacy-watch --watch src --exec \"ts-node src/index.ts\""
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@serlo/markdown": "^0.0.0",
    "body-parser": "^1.0.0",
    "express": "^4.0.0"
  },
  "devDependencies": {
    "@serlo/docker": "^0.0.0",
    "@serlo/node-image": "^0.0.0",
    "@serlo/workspace-image": "^0.0.0",
    "@types/express": "^4.0.0"
  },
  "private": true
}
