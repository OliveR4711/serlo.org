{
  "name": "@serlo-org/athene2",
  "version": "0.2.1",
  "license": "Apache-2.0",
  "author": "Serlo Education e.V.",
  "scripts": {
    "build": "ts-node scripts/build",
    "c": "docker-compose run --rm composer composer",
    "c-dev": "docker-compose run --rm composer-dev-tools composer",
    "prestart": "yarn c install && yarn c-dev install",
    "start": "docker-compose up --build -d",
    "stop": "docker-compose down",
    "format": "yarn _php-cs-fixer",
    "e2e": "jest",
    "unit": "docker-compose exec php src/vendor/bin/phpunit",
    "_php-cs-fixer": "docker-compose exec php dev-tools/vendor/bin/php-cs-fixer fix --config=php-cs-fixer.config.php",
    "_prettier": "prettier \"{{.circleci,__tests__,scripts}/**/*,*}.{js,jsx,ts,tsx,css,scss,sass,less,json,md,markdown,yaml,yml}\""
  },
  "dependencies": {},
  "devDependencies": {
    "@serlo/docker": "^0.0.0",
    "@types/expect-puppeteer": "^3.0.0",
    "@types/jest-environment-puppeteer": "^4.0.0",
    "@types/puppeteer": "^1.0.0",
    "axios": "^0.19.0",
    "jest-puppeteer": "^4.0.0",
    "npm-run-all": "^4.0.0",
    "pptr-testing-library": "^0.4.0",
    "puppeteer": "^1.0.0",
    "ramda": "^0.26.0",
    "ts-jest": "^24.0.0"
  },
  "private": true
}
